#%Module

module-whatis   "Load system GCC 5.4.0"
conflict compilers

set VERSION 5.4.0

setenv GNU_VERSION $VERSION
setenv GCC_VERSION $VERSION
setenv GCC_PATH /usr/bin

setenv     COMPILER_NAME  gnu
setenv     COMPILER_VER   $VERSION
setenv     CC   gcc
setenv     CXX  g++
setenv     FC   gfortran
setenv     F77  gfortran
